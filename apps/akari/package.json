    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-notifications": "~0.32.11",
    "expo-router": "~6.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.11",
    "expo-video": "~3.0.11",
    "expo-web-browser": "~15.0.7",
    "hls.js": "^1.6.7",
    "i18n-js": "^4.5.1",
    "libretranslate-api": "*",
    "pseudo-localization": "^3.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intl": "^7.1.11",
    "react-native": "0.81.4",
