  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.12",
    "jsdom": "^27.0.0",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.9.2",
    "vitest": "^2.1.9",
    "vitest-react-native": "^0.1.5"
  },
  "private": true
